.page-article
  
  .postArticle-content
    font-family: "jaf-bernino-sans","Lucida Grande","Lucida Sans Unicode","Lucida Sans",Geneva,Verdana,sans-serif
    letter-spacing: -0.02em
    font-weight: 400
    font-style: normal
    text-rendering: optimizeLegibility
    -webkit-font-smoothing: antialiased
    -moz-osx-font-smoothing: grayscale
    -moz-font-feature-settings: "liga" on
    color: rgba(0,0,0,0.8)
    font-size: 18px
    line-height: 1.4
  

  .postArticle-content
    position: relative
    font-size: 22px
    line-height: 1.5
    margin-bottom: 30px
    font-family: "freight-text-pro",Georgia,Cambria,"Times New Roman",Times,serif
    letter-spacing: .01rem
    font-weight: 400
    font-style: normal
  

  hr
    box-sizing: content-box
    height: 0
  

  .section--last
    padding-bottom: 5px
  

  .postArticle-content>section
    position: relative
    clear: both
  

  .postArticle-content, .postArticle-content:focus
    outline: 0
    word-break: break-word
    word-wrap: break-word
  

  .graf--hr, hr.section-divider
    display: block
    width: 100px
    margin: 51px auto 41px auto
    border: 0
    border-top: 1px solid rgba(0,0,0,0.15)
  

  .graf--p, .graf--blockquote, .graf--pullquote
    margin-bottom: 30px
  

  p[data-align="center"], .graf--h2[data-align="center"], .graf--h3[data-align="center"], .graf--h4[data-align="center"], .graf--blockquote[data-align="center"]
    text-align: center
  

  .graf--h3
    font-family: "jaf-bernino-sans","Lucida Grande","Lucida Sans Unicode","Lucida Sans",Geneva,Verdana,sans-serif
    letter-spacing: -0.02em
    font-weight: 700
    font-style: normal
    font-size: 36px
    margin-left: -1.8px
    line-height: 1.2
    margin-top: 40px
    margin-bottom: 4px
    color: rgba(0,0,0,0.8)
  
  .graf--h4
    font-family: "jaf-bernino-sans","Lucida Grande","Lucida Sans Unicode","Lucida Sans",Geneva,Verdana,sans-serif
    letter-spacing: -0.02em
    font-weight: 300
    font-style: normal
    font-size: 32px
    margin-left: -1.6px
    line-height: 1.2
    color: rgba(0,0,0,0.44)
    margin-top: 40px
    margin-bottom: 2px
  

  .graf--h2+.graf--p.graf--empty+.graf--h3, .graf--h3+.graf--p.graf--empty+.graf--h3, .graf--h4+.graf--p.graf--empty+.graf--h3, .graf--h2+.graf--p.graf--empty+.graf--h4, .graf--h3+.graf--p.graf--empty+.graf--h4, .graf--h4+.graf--p.graf--empty+.graf--h4
    margin-top: -8px
  

  .graf--p, .graf--blockquote, .graf--pullquote
    margin-bottom: 30px
  
  .graf-image, .graf-imageAnchor, .iframeContainer>iframe, .iframeContainer
    display: block
    margin: auto
    max-width: 100%
    box-sizing: border-box
  
  .aspectRatioPlaceholder
    position: relative
    width: 100%
    margin: 0 auto

  .aspectRatioPlaceholder.is-locked .graf-image, .aspectRatioPlaceholder.is-locked .graf-imageAnchor
    position: absolute
    top: 0
    left: 0
    width: 100%
    height: 100%
  
  .graf--figure
    position: relative
    margin-bottom: 30px
    clear: both
    outline: none
    box-sizing: border-box

  .graf--pullquote
    line-height: 1.4
    text-align: center
    font-size: 32px
    margin: 48px -160px
    border: none
    padding: 0
    font-family: "freight-text-pro",Georgia,Cambria,"Times New Roman",Times,serif
    letter-spacing: .01rem
    font-weight: 400
    font-style: italic
    transition: margin 100ms
  

  .section-captionContent, section.is-darkBackgrounded>.section-captionContent
    position: absolute
    top: 100%
    width: 100%
    display: block
    color: rgba(0,0,0,0.6)
    left: 0
    margin-top: 10px
  

  .section-captionContent, section.is-darkBackgrounded>.section-captionContent
    position: absolute
    top: 100%
    width: 100%
    display: block
    color: rgba(0,0,0,0.6)
    left: 0
    margin-top: 10px
  

  .graf--sectionCaption
    display: block
    font-size: 16px
    font-style: italic
    text-align: center
  

  .section-image--aspectRatioViewport .graf--last
    margin-bottom: 0
  

  .sectionLayout--outsetColumn, .sectionLayout--outsetRow
    max-width: 1020px
    width: 100%
    margin: 0 auto
    padding-top: 35px
    box-sizing: border-box
  
  div.section-divider, .section-inner, .postFollowUps, .notesPositionContainer
    position: relative
    right: 0%
    transition: right 200ms
  
  .sectionLayout--outsetColumn+.layoutSingleColumn, .sectionLayout--outsetColumn+.u-sizeFullWidth, .sectionLayout--outsetRow+.layoutSingleColumn, .sectionLayout--outsetRow+.u-sizeFullWidth
    padding-top: 65px

  .layoutSingleColumn
    width: 700px
    margin: 0 auto
  
  div.section-divider, .section-inner, .postFollowUps, .notesPositionContainer
    position: relative
    right: 0%
    transition: right 200ms
  
  .sectionLayout--outsetColumn:before, .sectionLayout--outsetRow:before, .sectionLayout--outsetColumn:after, .sectionLayout--outsetRow:after
    display: table
    content: " "
  
  .sectionLayout--outsetColumn:after, .sectionLayout--outsetRow:after
    clear: both

  figure
    margin: 1em 40px
  
  .postArticle-content
    position: relative
    font-size: 22px
    line-height: 1.5
    margin-bottom: 30px
    font-family: "freight-text-pro",Georgia,Cambria,"Times New Roman",Times,serif
    letter-spacing: .01rem
    font-weight: 400
    font-style: normal
  

  ::selection
    background-color: #cce2ff
    color: rgba(0, 0, 0, 0.8)


  *, *:before, *:after
    box-sizing: inherit
  

  div.section-divider, .section-inner, .postFollowUps, .notesPositionContainer
    position: relative
    right: 0%
    transition: right 200ms
  
  figcaption
    display: block
  

  article, aside, details, figcaption, figure, footer, header, hgroup, main, nav, section, summary
    display: block
  

  section:first-child>.section-divider, section.is-backgrounded+section>.section-divider
    display: none
  
  
  .ui.comments
    width: 700px
    margin: 0 auto
